<FrameLayout
    android:id="@+id/loginakun"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="917dp"
    android:background="@drawable/container_android_compact">

    <FrameLayout
        android:id="@+id/container_group"
        android:layout_width="315dp"
        android:layout_height="52dp"
        android:layout_marginRight="48dp"
        android:layout_marginTop="768dp"
        android:layout_gravity="top|right">

        <ImageView
            android:id="@+id/image_rectangle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="108dp"
            android:background="@drawable/image_rectangle"
            android:scaleType="centerCrop" />

        <ImageView
            android:id="@+id/image_rectangle1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="108dp"
            android:layout_marginRight="108dp"
            android:background="@drawable/image_rectangle1"
            android:scaleType="centerCrop" />

        <ImageView
            android:id="@+id/image_rectangle2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="108dp"
            android:scaleType="centerCrop"
            android:src="@drawable/image_rectangle2" />

        <ImageView
            android:id="@+id/image_social_media_x"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginLeft="252dp"
            android:layout_marginRight="38dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="14dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_social_media_x" />

        <ImageView
            android:id="@+id/image_social_media_x1"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_marginLeft="253dp"
            android:layout_marginRight="37dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="14dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_social_media_x1" />

        <ImageView
            android:id="@+id/image_facebook_logo"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_marginLeft="132dp"
            android:layout_marginRight="1dp"
            android:layout_marginTop="1dp"
            android:layout_gravity="top"
            android:scaleType="centerInside"
            android:src="@drawable/image_facebook_logo" />

        <ImageView
            android:id="@+id/image_facebook_logo1"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_marginLeft="133dp"
            android:layout_marginRight="133dp"
            android:layout_marginTop="1dp"
            android:layout_gravity="top"
            android:scaleType="centerInside"
            android:src="@drawable/image_facebook_logo1" />

        <ImageView
            android:id="@+id/image_google_logo"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:layout_gravity="left"
            android:scaleType="centerInside"
            android:src="@drawable/image_google_logo" />

        <ImageView
            android:id="@+id/image_google_logo1"
            android:layout_width="26dp"
            android:layout_height="26dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="13dp"
            android:layout_marginBottom="13dp"
            android:layout_gravity="left"
            android:scaleType="centerInside"
            android:src="@drawable/image_google_logo1" />
    </FrameLayout>

    <EditText
        android:id="@+id/edit_email"
        android:layout_width="315dp"
        android:layout_height="52dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="377dp"
        android:layout_gravity="top|center_horizontal"
        android:hint="Enter your email"
        android:inputType="textEmailAddress"
        android:background="@drawable/image_rectangle3"
        android:padding="10dp"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <EditText
        android:id="@+id/edit_password"
        android:layout_width="315dp"
        android:layout_height="52dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="447dp"
        android:layout_gravity="top|center_horizontal"
        android:hint="Enter your password"
        android:inputType="textPassword"
        android:background="@drawable/image_rectangle4"
        android:padding="10dp"
        android:textColor="#FF000000"
        android:textSize="13dp" />


    <ImageView
        android:id="@+id/image_rectangle5"
        android:layout_width="315dp"
        android:layout_height="52dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="535dp"
        android:layout_gravity="top|center_horizontal"
        android:background="@drawable/image_rectangle5"
        android:scaleType="centerInside" />

    <TextView
        android:id="@+id/text_login_account"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="329.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="top|left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Login to your Account"
        android:textColor="#FF000000"
        android:textSize="20dp" />



    <TextView
        android:id="@+id/text_or_sign_up"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginTop="726.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="- Or sign up with -"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/text_sign_up_prompt"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="615.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Don,t have a account? Sign up"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/text_sign_in"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginTop="552.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Sign in"
        android:textColor="#FFFFFFFF"
        android:textSize="13dp" />

    <TextView
        android:id="@+id/text_logo"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="141.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Logo"
        android:textColor="#FF000000"
        android:textSize="13dp" />

    <ImageView
        android:id="@+id/image_facebook_logo2"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginLeft="0.5dp"
        android:layout_marginTop="769dp"
        android:layout_gravity="top|center_horizontal"
        android:scaleType="centerInside"
        android:src="@drawable/image_facebook_logo2" />
</FrameLayout>
