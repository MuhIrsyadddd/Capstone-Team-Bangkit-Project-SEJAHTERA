<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/popup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/container_android_compact">

    <ImageView
        android:id="@+id/image_fork_spoon"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="498dp"
        android:layout_marginTop="419dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_fork_spoon" />

    <ImageView
        android:id="@+id/image_lipstick"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="498dp"
        android:layout_marginTop="480dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_lipstick" />

    <ImageView
        android:id="@+id/image_gas_station"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="498dp"
        android:layout_marginTop="541dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_gas_station" />

    <ImageView
        android:id="@+id/image_investment_selection"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginLeft="498dp"
        android:layout_marginTop="602dp"
        android:layout_gravity="top|left"
        android:scaleType="centerInside"
        android:src="@drawable/image_investment_selection" />


    <EditText
        android:id="@+id/edit_nama_catatan"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/image_rectangle33b"
        android:gravity="left|center_vertical"
        android:hint="Nama Catatan"
        android:inputType="text"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:textColor="#FF000000"
        android:textSize="13sp"
        android:visibility="visible" />

    <EditText
        android:id="@+id/edit_nominal_pengeluaran"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="180dp"
        android:layout_gravity="top|left"
        android:gravity="left|center_vertical"
        android:hint="Nominal Pengeluaran"
        android:inputType="number"
        android:background="@drawable/image_rectangle33b"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:textColor="#FF000000"
        android:textSize="13sp" />



    <ImageView
        android:id="@+id/image_vector5"
        android:layout_width="23.62dp"
        android:layout_height="23.62dp"
        android:layout_marginLeft="310dp"
        android:layout_marginRight="53.38dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="687.38dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_vector5" />

    <ImageView
        android:id="@+id/image_rectangle5"
        android:layout_width="78dp"
        android:layout_height="30dp"
        android:layout_marginLeft="91dp"
        android:layout_marginTop="240dp"
        android:text="Button"
        android:layout_gravity="top|center_horizontal"
        android:background="@drawable/image_rectangle66c"
        android:scaleType="centerInside" />

    <TextView
        android:id="@+id/text_buat"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginLeft="90.5dp"
        android:layout_marginTop="245.5dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Buat"
        android:textColor="#FFFFFFFF"
        android:textSize="12dp" />

    <ImageView
        android:id="@+id/image_rectangle6"
        android:layout_width="128dp"
        android:layout_height="36dp"
        android:layout_marginRight="67dp"
        android:layout_marginTop="80dp"
        android:layout_gravity="top|center_horizontal"
        android:background="@drawable/image_rectangle66c"
        android:scaleType="centerInside" />

    <ImageView
        android:id="@+id/image_rectangle7"
        android:layout_width="128dp"
        android:layout_height="36dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="80dp"
        android:layout_gravity="top|center_horizontal"
        android:background="@drawable/image_rectangle7"
        android:scaleType="centerInside" />

    <TextView
        android:id="@+id/text_catatan"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginRight="67dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Catatan "
        android:textColor="#FFFFFF"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/text_tabungan"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginLeft="67.5dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="-0.5dp"
        android:layout_gravity="center_horizontal"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Tabungan"
        android:textColor="#FF00AB6B"
        android:textSize="12dp" />
</FrameLayout>
